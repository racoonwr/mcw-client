<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mcw">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:name=".demo.DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".demo.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".demo.ui.activity.CreateMeetingActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".demo.ui.activity.SelectionViewActivity" />
        <activity android:name=".demo.ui.activity.PreviewMeetingActivity" />
        <activity android:name=".demo.ui.activity.MeetingDetailActivity" />
        <activity android:name=".demo.ui.activity.MyVoteActivity" />
        <activity android:name=".demo.ui.activity.VoteDetailActivity" />
        <activity android:name=".demo.ui.activity.CreateVoteActivity" />
        <activity android:name=".demo.ui.activity.ScanQRCodeActivity" />
        <activity android:name=".demo.ui.activity.ChartDetailActivity" />
        <activity android:name=".demo.ui.activity.ShowQRCodeActivity" />
        <activity
            android:name=".demo.ui.activity.UploadMeetingSummaryActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".demo.ui.activity.UserPickerActivity" />
        <activity
            android:name=".demo.ui.activity.MeetingActivity"
            android:windowSoftInputMode="adjustPan"></activity>
        <activity
            android:name=".demo.util.rxjavaresult.ShadowActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
    </application>

</manifest>